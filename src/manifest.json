{
  "manifest_version": 2,
  "name": "chrome extension sample",
  "short_name": "chrome extension",
  "description": "testing chrome extension",
  "version": "0.0.14",
  "version_name": "0.0.14",
  "icons": {
    "36": "assets/icon_36.png"
  },
  "background": {
        "scripts": ["background.js"]
   },
   "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
       "36": "assets/icon_36.png"
   }
   },
    "permissions": [
    "activeTab",
    "storage",
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage"
  ]
}

